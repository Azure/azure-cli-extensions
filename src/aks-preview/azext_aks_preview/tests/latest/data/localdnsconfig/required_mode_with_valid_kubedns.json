{
  "mode": "Required",
  "kubeDnsOverrides": {
    ".": {
      "cacheDurationInSeconds": 3600,
      "forwardDestination": "ClusterCoreDNS",
      "forwardPolicy": "Sequential",
      "maxConcurrent": 1000,
      "protocol": "PreferUDP",
      "queryLogging": "Error",
      "serveStale": "Verify",
      "serveStaleDurationInSeconds": 3600
    },
    "cluster.local": {
      "cacheDurationInSeconds": 3600,
      "forwardDestination": "ClusterCoreDNS",
      "forwardPolicy": "Sequential",
      "maxConcurrent": 1000,
      "protocol": "ForceTCP",
      "queryLogging": "Error",
      "serveStale": "Immediate",
      "serveStaleDurationInSeconds": 3600
    }
  }
}
