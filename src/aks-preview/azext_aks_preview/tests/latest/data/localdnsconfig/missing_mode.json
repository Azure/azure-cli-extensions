{
  "kubeDnsOverrides": {
    ".": {
      "cacheDurationInSeconds": 3600,
      "forwardDestination": "ClusterCoreDNS",
      "forwardPolicy": "Sequential",
      "maxConcurrent": 1000,
      "protocol": "PreferUDP",
      "queryLogging": "Error",
      "serveStale": "Verify",
      "serveStaleDurationInSeconds": 3600
    }
  },
  "vnetDnsOverrides": {
    ".": {
      "cacheDurationInSeconds": 3600,
      "forwardDestination": "VnetDNS",
      "forwardPolicy": "Sequential",
      "maxConcurrent": 1000,
      "protocol": "PreferUDP",
      "queryLogging": "Error",
      "serveStale": "Verify",
      "serveStaleDurationInSeconds": 3600
    }
  }
}
